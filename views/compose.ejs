<%- include('header')%>
<h1>Compose A Post</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="title" class="">title</label>
    <input type="text" class = "form-control" name="title"  class="col-md-12" value="">
  </div>
  <div class="form-group">
    <label for="content" class="">content</label>
    <textarea class="form-control" name="content" rows="15" cols"80"></textarea>
  </div>
  <button type="submit" class = "btn-primary" name="button">Publish</button>

</form>
<%- include('footer')%>
